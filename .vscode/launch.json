            "args": ["-hmc", "9.12.35.134", "-cpc", "T257"],
            "console": "integratedTerminal"
        },
        {
            "name": "partition back t257",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/parsBackup.py",
